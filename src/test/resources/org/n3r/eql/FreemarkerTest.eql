-- import org/n3r/eql/SimpleTest.eql *TestTable

-- [setup onerr=resume]
create table demo_attr (id varchar2(32), attr varchar2(64))
-- [teardown]
drop table demo_attr

-- [selectIf returnType=org.n3r.eql.SimpleTest$Bean]
SELECT A,B,C,D,E
FROM ESQL_TEST
WHERE A = #a#
AND
<#if e == 100>
 C = #c#
<#elseif e == 200>
 C = 'AC'
<#else>
 C = 'FALSE'
</#if>

-- [testInner]
SELECT 'X'
FROM DUAL
<#if _databaseId == "oracle">
WHERE 'X' = 'Y'
</#if>

-- [testIn]
select * from dual
where 1 in (''
<#list lst as x>
  ,#lst[${x_index}]#
</#list>
)


-- [testInsertAll]
insert all
<#list arr as x>
 into demo_attr(id, attr)
 values(#id#, #arr[${x_index}]#)
</#list>
select 1 from dual
